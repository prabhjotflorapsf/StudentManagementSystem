            @Html.DisplayFor(modelItem => item.TelephoneNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.batch.Batch)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.course.Course)
